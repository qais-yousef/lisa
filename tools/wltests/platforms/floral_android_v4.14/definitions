PLATFORM_NAME="Floral"
WLTEST_REPO_TARGET="y"
REPO_KERNEL_PATH="private/msm-google"

# Artifacts generated by Google's build.sh script
OUT_DIR_COMMON="$KERNEL_SRC/out/"
KERNEL_BRANCH=$(git --git-dir="$KERNEL_SRC/$REPO_KERNEL_PATH/.git"  rev-parse --abbrev-ref HEAD)
BRANCH=$(echo $KERNEL_BRANCH | tr / _)
ARTIFACTS_PATH="$KERNEL_SRC/out/$BRANCH/dist"
KERNEL_PATH="$KERNEL_SRC/out/$BRANCH/private/msm-google/arch/arm64/boot"
KERNEL_IMAGE="${KERNEL_IMAGE:-Image.lz4-dtb}"
ANDROID_DTBO_IMAGE="${ANDROID_DTBO_IMAGE:-dtbo.img}"
